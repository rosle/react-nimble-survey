{
  "name": "react-nimble-survey",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=8.11.0",
    "node": ">=16.16.0"
  },
  "dependencies": {
    "axios": "0.21.1",
    "bootstrap": "5.2.0",
    "i18next": "21.6.14",
    "i18next-browser-languagedetector": "6.1.3",
    "i18next-http-backend": "1.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "11.16.1",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "sass": "1.49.11",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "7.18.12",
    "@cypress/code-coverage": "3.10.0",
    "@cypress/instrument-cra": "1.4.0",
    "@cypress/webpack-dev-server": "2.0.0",
    "@nimblehq/eslint-config-nimble-react": "1.1.0",
    "@nimblehq/stylelint-config-nimble": "1.0.2",
    "@testing-library/cypress": "8.0.3",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.3.0",
    "@types/jest": "28.1.6",
    "@types/node": "18.6.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "babel-plugin-istanbul": "6.1.1",
    "cypress": "10.5.0",
    "cypress-react-selector": "3.0.0",
    "danger": "10.9.0",
    "danger-plugin-istanbul-coverage": "1.6.2",
    "eslint": "8.11.0",
    "prettier": "2.6.0",
    "start-server-and-test": "1.14.0",
    "stylelint": "14.9.1",
    "typescript": "4.7.4"
  },
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:coverage": "react-scripts test --coverage --watchAll=false && npm run cypress:all && node ./scripts/coverage-merge.js && nyc report",
    "test:merge-coverage": "node ./scripts/coverage-merge.js",
    "lint": "eslint ./ --ext .ts,.tsx",
    "lint:fix": "eslint ./src ./cypress --ext .ts,.tsx --fix",
    "stylelint": "stylelint '**/*.scss'",
    "stylelint:fix": "stylelint '**/*.scss' --fix",
    "codebase:lint": "npm run lint && npm run stylelint",
    "codebase:fix": "npm run lint:fix && npm run stylelint:fix",
    "cypress:all": "npm run cypress:component && npm run cypress:e2e",
    "cypress:component": "cypress run --component --quiet",
    "cypress:e2e": "cypress run --quiet",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.cy.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "json"
    ]
  },
  "nyc": {
    "report-dir": "coverage/cypress",
    "exclude": [
      "src/reportWebVitals.ts",
      "cypress/support/*"
    ],
    "excludeAfterRemap": true
  }
}
